# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Contact Form Configuration
VITE_CONTACT_EMAIL=your-email@example.com
VITE_FORMSUBMIT_ENDPOINT=https://formsubmit.co/your-email@example.com

# Google Services
VITE_GOOGLE_SITE_VERIFICATION=your-google-site-verification-code

# Social Media Links
VITE_LINKEDIN_URL=https://linkedin.com/in/your-profile
VITE_GITHUB_URL=https://github.com/your-username
VITE_INSTAGRAM_URL=https://instagram.com/your-username
VITE_YOUTUBE_URL=https://youtube.com/@your-channel

# Website Metadata
VITE_SITE_URL=https://your-domain.com
VITE_SITE_NAME=Your Name
VITE_SITE_DESCRIPTION=Your site description

# Optional: Analytics and Tracking
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX
